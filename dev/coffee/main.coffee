class root.MY_CLASS.main extends root.MY_CLASS.base
	
	
	# -----------------------------------------------
	# コンストラクタ
	# -----------------------------------------------
	constructor: ->
		
		# コンテンツ
		@_con;
		
		super();
		


	# -----------------------------------------------
	# セットアップ
	# -----------------------------------------------
	setup: =>
		
		# コンテンツ
		@_con = new root.MY_CLASS.contentsView($("#" + root.MY.conf.ID));
		@_con.setup();
		
		
	# -----------------------------------------------
	# コンテンツ取得
	# -----------------------------------------------
	con: =>
		
		return @_con;
				